        error_flag = cmor_zfactor(       &
             zaxis_id=ilev,        &
             zfactor_name='a',           &
             axis_ids= (/ilev/),   &
             units=' ',                  &
             zfactor_values=ah_coeff)
        zfactor_id = cmor_zfactor(       &
             zaxis_id=ilev,        &
             zfactor_name='ps',          &
             axis_ids=(/axis_ids(1),axis_ids(2),axis_ids(3)/), &
             units='Pa')
        axis_ids(idim) = ilev
        write(*,'('' dimension: '',a,'' defined: '',i4)') 'standard_hybrid_sigma',axis_ids(idim)
        idim = idim + 1
     end select
  enddo
