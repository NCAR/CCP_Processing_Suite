#!/bin/sh
#
# Store data on archival system
#
if [ $# -ne 0 ] ; then
  echo "Usage: store_to_archive"
  exit 1
fi
#
cd $CACHEDIR
ln -s -f ${BASEDIR}/mss_write .
#
LAST=`ls -1 *cat*nc | tail -1`
#
while [ -f $LAST ] ; do
  for g in `ls *cat*nc | head -5` ; do
    ./mss_write $g $MSSPROC &
  done
  wait
done
